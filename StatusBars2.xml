                        <Offset>
                            <AbsDimension x="87" y="0"/>
                        </Offset>
                    </Anchor>
                </Anchors>
            </Button>

            <Button name="$parent_RuneButton3" inherits="RuneButtonIndividualTemplate" id="3">
