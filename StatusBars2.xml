    <!-- Aura bar template -->
    <Frame name="StatusBars2_AuraBarTemplate" movable="true" virtual="true" >
        <Size x="170" y="20" />
    </Frame>

    <!-- Eclipse bar template -->
	<Frame name="StatusBars2_EclipseBarTemplate" frameStrata="LOW" toplevel="true" hidden="true" moveable="true" virtual="true">
        <Animations>
            <AnimationGroup parentKey="sunActivate">
                <Alpha target="$parentSunBar"        change="1" duration="0.6" order="1"/>
                <Alpha target="$parentGlow"          change="1" duration="0.6" order="1"/>
                <Alpha target="$parentDarkMoon"  change="1" duration="0.6" order="1"/>
                <Scripts>
                    <OnPlay>
                        self:GetParent().moonBar:SetAlpha(0);
                        self:GetParent().darkSun:SetAlpha(0);
                    </OnPlay>
                    <OnFinished>
                        self:GetParent().sunBar:SetAlpha(1);
                        self:GetParent().glow:SetAlpha(1);
                        self:GetParent().darkMoon:SetAlpha(1);
                        self:GetParent().glow.pulse:Play();
                    </OnFinished>
                </Scripts>
            </AnimationGroup>
            <AnimationGroup parentKey="sunDeactivate">
                <Alpha target="$parentSunBar"        change="-1" duration="0.6" order="1"/>
                <Alpha target="$parentGlow"          change="-1" duration="0.6" order="1"/>
                <Alpha target="$parentDarkMoon"  change="-1" duration="0.6" order="1"/>
                <Scripts>
                    <OnPlay>
                        self:GetParent().glow.pulse:Stop();
                    </OnPlay>
                    <OnFinished>
                        self:GetParent().sunBar:SetAlpha(0);
                        self:GetParent().glow:SetAlpha(0);
                        self:GetParent().darkMoon:SetAlpha(0);
                    </OnFinished>
                </Scripts>
            </AnimationGroup>
            <AnimationGroup parentKey="moonActivate">
                <Alpha target="$parentMoonBar"   change="1" duration="0.6" order="1"/>
                <Alpha target="$parentGlow"      change="1" duration="0.6" order="1"/>
                <Alpha target="$parentDarkSun"   change="1" duration="0.6" order="1"/>
                <Scripts>
                    <OnPlay>
                        self:GetParent().sunBar:SetAlpha(0);
                        self:GetParent().darkMoon:SetAlpha(0);
                    </OnPlay>
                    <OnFinished>
                        self:GetParent().moonBar:SetAlpha(1);
                        self:GetParent().glow:SetAlpha(1);
                        self:GetParent().darkSun:SetAlpha(1);
                        self:GetParent().glow.pulse:Play();
                    </OnFinished>
                </Scripts>
            </AnimationGroup>
            <AnimationGroup parentKey="moonDeactivate">
                <Alpha target="$parentMoonBar"   change="-1" duration="0.6" order="1"/>
                <Alpha target="$parentGlow"      change="-1" duration="0.6" order="1"/>
                <Alpha target="$parentDarkSun"   change="-1" duration="0.6" order="1"/>
                <Scripts>
                    <OnPlay>
                        self:GetParent().glow.pulse:Stop();
                    </OnPlay>
                    <OnFinished>
                        self:GetParent().moonBar:SetAlpha(0);
                        self:GetParent().glow:SetAlpha(0);
                        self:GetParent().darkSun:SetAlpha(0);
                    </OnFinished>
                </Scripts>
            </AnimationGroup>
        </Animations>
        <Size x="140" y="38"/>
        <Anchors>
          <Anchor point="TOP" relativeTo="PlayerFrame" relativePoint="BOTTOM">
            <Offset>
              <AbsDimension x="48" y="40"/>
            </Offset>
          </Anchor>
        </Anchors>
        <HitRectInsets>
            <AbsInset left="4" right="4" top="6" bottom="6"/>
        </HitRectInsets>
        <Layers>
            <Layer level="ARTWORK">
                <Texture name="$parentBar" atlas="DruidEclipse-BaseBar" useAtlasSize="true">
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND">
                <Texture parentKey="Sun" name="$parentSun" atlas="DruidEclipse-BaseSun" useAtlasSize="true">
                    <Anchors>
                        <Anchor point="CENTER" relativeTo="$parentBar"  relativePoint="RIGHT" x="-17" y="1"/>
                    </Anchors>
                </Texture>
                <Texture parentKey="Moon" name="$parentMoon" atlas="DruidEclipse-BaseMoon" useAtlasSize="true">
                    <Anchors>
                        <Anchor point="CENTER" relativeTo="$parentBar" relativePoint="LEFT" x="17" y="1"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="1">
                <Texture parentKey="DarkSun" name="$parentDarkSun" atlas="DruidEclipse-LunarSun" useAtlasSize="true">
                    <Anchors>
                        <Anchor point="CENTER" relativeTo="$parentSun"/>
                    </Anchors>
                </Texture>
                <Texture parentKey="DarkMoon" name="$parentDarkMoon" atlas="DruidEclipse-SolarMoon" useAtlasSize="true">
                    <Anchors>
                        <Anchor point="CENTER" relativeTo="$parentMoon"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK" textureSubLevel="1">
                <Texture parentKey="SunBar" name="$parentSunBar" atlas="DruidEclipse-SolarBar" useAtlasSize="true">
                    <Anchors>
                        <Anchor point="CENTER" x="15"/>
                    </Anchors>
                </Texture>
                <Texture parentKey="MoonBar" name="$parentMoonBar" atlas="DruidEclipse-LunarBar" useAtlasSize="true">
                    <Anchors>
                        <Anchor point="CENTER" x="-15"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY" textureSubLevel="-1">
                <Texture parentKey="MoonCover" name="$parentMoonCover" atlas="DruidEclipse-SolarCover" useAtlasSize="true" hidden="true">
                    <Anchors>
                        <Anchor point="LEFT" x="20" y="1"/>
                    </Anchors>
                </Texture>
                <Texture parentKey="SunCover" name="$parentSunCover" atlas="DruidEclipse-LunarCover" useAtlasSize="true" hidden="true">
                    <Anchors>
                        <Anchor point="RIGHT" x="-30"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
                <Texture parentKey="Marker" name="$parentMarker" atlas="DruidEclipse-Arrow" useAtlasSize="true" alphaMode="ADD">
                    <Anchors>
                        <Anchor point="CENTER" y="2"/>
                    </Anchors>
                </Texture>
                <Texture parentKey="Glow" name="$parentGlow" atlas="DruidEclipse-SolarSun">
                    <Size x="43" y="45"/>
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                </Texture>
                <FontString name="$parentPowertext" inherits="TextStatusBarText" justifyH="CENTER" parentKey="PowerText" hidden="true">
                    <Anchors>
                        <Anchor point="CENTER" x="0" y="0"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
		<Scripts>
			<OnShow function="StatusBars2_EclipseBar_OnShow"/>
		</Scripts>
	</Frame>

    <Frame name="StatusBars2_GroupFrameTemplate" movable="true" hidden="false" frameStrata="LOW" virtual = "true" >
        <Size x="170" y="50" />
    </Frame>

    <!-- Main frame -->
    <Frame name="StatusBars2" parent="UIParent" inherits="StatusBars2_GroupFrameTemplate" >

        <Size x="170" y="200" />

        <Anchors>
            <Anchor point="CENTER" relativeto="$Parent" >
                <Offset x="85" y="-282" />
            </Anchor>
        </Anchors>

		<Scripts>
            <OnLoad>
                StatusBars2_OnLoad( self );
            </OnLoad>
        </Scripts>
    </Frame>

    <!-- Bar button template -->
    <Button name="StatusBar2_BarOptionsButtonTemplate" virtual="true" >

        <Size x="140" y="20" />

        <ButtonText name="$parentText">
            <Size x="140" y="13"/>
            <Anchors>
                <Anchor point="LEFT" relativeTo="$parent" >
                    <Offset>
                        <AbsDimension x="10" y="1"/>
                    </Offset>
                </Anchor>
            </Anchors>
        </ButtonText>

        <NormalFont style="GameFontNormalLeft"/>
        <HighlightFont style="GameFontHighlightLeft"/>
        <DisabledFont style="GameFontDisableLeft"/>

        <HighlightTexture file="Interface\QuestFrame\UI-QuestLogTitleHighlight" alphaMode="ADD" >
        </HighlightTexture>

        <Scripts>
            <OnClick>
                StatusBars2_BarOptionsButton_OnClick( self );
            </OnClick>
        </Scripts>
    </Button>

    <!-- Options frame -->
    <Frame name="StatusBars2_Options" >
        <Scripts>
            <OnLoad>
                StatusBars2_Options_OnLoad( self );
            </OnLoad>
        </Scripts>
        <Layers>
            <Layer level="BACKGROUND">
                <FontString name="$parent_TextDisplayOptionsLabel" inherits="GameFontNormalSmallLeft" text="Text Display Options" >
                    <Size x="200" y="17"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativeTo="$Parent" >
                            <Offset>
                                <AbsDimension x="-140" y="-20"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parent_TextSizeLabel" inherits="GameFontNormalSmallLeft" text="Text Size" >
                    <Size x="200" y="17"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativeTo="$parent_TextDisplayOptionsLabel" >
                            <Offset>
                                <AbsDimension x="0" y="-60"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <CheckButton name="$parent_FadeButton" inherits="OptionsCheckButtonTemplate" >
                <Anchors>
                    <Anchor point="TOPLEFT" relativeto="$parent" >
                        <Offset x="10" y="-10" />
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        getglobal( self:GetName( ) .. "Text" ):SetText( "Fade bars in and out" );
                    </OnLoad>
                </Scripts>
            </CheckButton>
            <CheckButton name="$parent_LockedButton" inherits="OptionsCheckButtonTemplate" >
                <Anchors>
                    <Anchor point="TOPLEFT" relativeto="$parent_FadeButton" >
                        <Offset x="0" y="-24" />
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        getglobal( self:GetName( ) .. "Text" ):SetText( "Lock bar positions" );
                    </OnLoad>
                </Scripts>
            </CheckButton>
            <CheckButton name="$parent_GroupedButton" inherits="OptionsCheckButtonTemplate" >
                <Anchors>
                    <Anchor point="TOPLEFT" relativeto="$parent_LockedButton" >
                        <Offset x="0" y="-24" />
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        getglobal( self:GetName( ) .. "Text" ):SetText( "Lock bars to groups" );
                    </OnLoad>
                </Scripts>
            </CheckButton>
            <CheckButton name="$parent_LockGroupsTogetherButton" inherits="OptionsCheckButtonTemplate" >
                <Anchors>
                    <Anchor point="TOPLEFT" relativeto="$parent_GroupedButton" >
                        <Offset x="0" y="-24" />
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        getglobal( self:GetName( ) .. "Text" ):SetText( "Lock groups together" );
                    </OnLoad>
                </Scripts>
            </CheckButton>
            <Button name="$parent_ResetBarPositionButton" inherits="OptionsButtonTemplate" text="Snap Bars To Group">
                <Size x="150" y="21"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeto="$parent_LockGroupsTogetherButton" >
                        <Offset x="0" y="-24" />
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                        StatusBars2_Options_ResetBarPositionButton_OnClick( self );
                    </OnClick>
                </Scripts>
            </Button>
            <Button name="$parent_ResetGroupPositionButton" inherits="OptionsButtonTemplate" text="Reset Group Positions">
                <Size x="150" y="21"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeto="$parent_ResetBarPositionButton" >
                        <Offset x="0" y="-24" />
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                        StatusBars2_Options_ResetGroupPositionButton_OnClick( self );
                    </OnClick>
                </Scripts>
            </Button>
			<Button name="$parent_MoveBarsButton" inherits="OptionsButtonTemplate" text="Configure Bars">
				<Size x="150" y="21"/>
				<Anchors>
					<Anchor point="TOPLEFT" relativeto="$parent_ResetGroupPositionButton" >
						<Offset x="0" y="-24" />
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						StatusBars2_Options_ToggleMoveBars_OnClick( self );
					</OnClick>
				</Scripts>
			</Button>
            <Frame name="$parent_TextDisplayOptionsMenu" inherits="UIDropDownMenuTemplate" >
                <Size x="170" y="17"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent_TextDisplayOptionsLabel" relativePoint="BOTTOMLEFT" >
                        <Offset x="-12" y="-3" />
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        UIDropDownMenu_Initialize( self, StatusBars2_TextDisplayOptionsMenu_Initialize );
                        UIDropDownMenu_SetWidth(self, self:GetWidth());
                        UIDropDownMenu_SetButtonWidth(self, "TEXT");
                    </OnLoad>
                </Scripts>
            </Frame>
            <Frame name="$parent_TextSizeMenu" inherits="UIDropDownMenuTemplate" >
                <Size x="170" y="17"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent_TextSizeLabel" relativePoint="BOTTOMLEFT" >
                        <Offset x="-12" y="-3" />
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        UIDropDownMenu_Initialize( self, StatusBars2_FontMenu_Initialize );
                        UIDropDownMenu_SetWidth(self, self:GetWidth());
                        UIDropDownMenu_SetButtonWidth(self, "TEXT");
                    </OnLoad>
                </Scripts>
            </Frame>
			<Slider name="$parent_ScaleSlider" inherits="OptionsSliderTemplate" minValue="0.25" maxValue="3.0" valueStep="0.05" >
                <Size x="200" y="17"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$Parent" >
                        <Offset x="85" y="-330" />
                    </Anchor>
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString name="$parentLabel" inherits="GameFontNormalSmallLeft" text="Scale">
                            <Size x="64" y="14"/>
                            <Anchors>
                                <Anchor point="RIGHT" relativePoint="LEFT">
                                    <Offset>
                                        <AbsDimension x="-4" y="1"/>
                                    </Offset>
                                </Anchor>
                            </Anchors>
                        </FontString>
                        <FontString name="$parentValueLabel" inherits="GameFontNormalSmallLeft" >
                            <Anchors>
                                <Anchor point="LEFT" relativeTo="$Parent" relativePoint="RIGHT">
                                    <Offset>
                                        <AbsDimension x="20" y="0"/>
                                    </Offset>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnLoad>
                        self.type = CONTROLTYPE_SLIDER;
                        getglobal( self:GetName( ) .. "Low" ):SetText( "0.25" );
                        getglobal( self:GetName( ) .. "High" ):SetText( "3.0" );
                    </OnLoad>
                    <OnValueChanged>
                        getglobal( self:GetName( ) .. "ValueLabel" ):SetText( StatusBars2_Round( value, 2 ) );
                    </OnValueChanged>
                </Scripts>
            </Slider>
            <Slider name="$parent_AlphaSlider" parentKey="slider" inherits="OptionsSliderTemplate" minValue="0.2" maxValue="1.0" valueStep="0.1" >
                <Size x="200" y="17"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$Parent" >
                        <Offset x="85" y="-370" />
                    </Anchor>
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString name="$parentLabel" inherits="GameFontNormalSmallLeft" text="Opacity">
                            <Size x="64" y="14"/>
                            <Anchors>
                                <Anchor point="RIGHT" relativePoint="LEFT">
                                    <Offset>
                                        <AbsDimension x="-4" y="1"/>
                                    </Offset>
                                </Anchor>
                            </Anchors>
                        </FontString>
                        <FontString name="$parentValueLabel" inherits="GameFontNormalSmallLeft" >
                            <Anchors>
                                <Anchor point="LEFT" relativeTo="$Parent" relativePoint="RIGHT">
                                    <Offset>
                                        <AbsDimension x="20" y="0"/>
                                    </Offset>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnLoad>
                        self.type = CONTROLTYPE_SLIDER;
                        getglobal( self:GetName( ) .. "Low" ):SetText( "20%" );
                        getglobal( self:GetName( ) .. "High" ):SetText( "100%" );
                    </OnLoad>
                    <OnValueChanged>
                        getglobal( self:GetName( ) .. "ValueLabel" ):SetText( StatusBars2_Round( value * 100, 0 ).."%" );
                    </OnValueChanged>
                </Scripts>
            </Slider>
        </Frames>
    </Frame>

</Ui>
