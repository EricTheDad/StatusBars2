            </Button>

            <Button name="$parent_RuneButton5" inherits="RuneButtonIndividualTemplate" id="5">
                <Anchors>
                    <Anchor point="LEFT" relativeTo="$Parent" relativePoint="LEFT">
                        <Offset>
                            <AbsDimension x="62" y="0"/>
                        </Offset>
                    </Anchor>
                </Anchors>
            </Button>

            <Button name="$parent_RuneButton6" inherits="RuneButtonIndividualTemplate" id="6">
                <Anchors>
